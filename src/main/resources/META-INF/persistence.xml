<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">

	<persistence-unit name="Jnoc_PU" transaction-type="JTA">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<jta-data-source>jdbc/JnocPro</jta-data-source>
		<class>com.jnoc.per.entity.Account</class>
		<class>com.jnoc.per.entity.Address</class>
		<class>com.jnoc.per.entity.Attachment</class>
		<class>com.jnoc.per.entity.Audit</class>
		<class>com.jnoc.per.entity.Comment</class>
		<class>com.jnoc.per.entity.Company</class>
		<class>com.jnoc.per.entity.Contact</class>
		<class>com.jnoc.per.entity.Filter</class>
		<class>com.jnoc.per.entity.Incident</class>
		<class>com.jnoc.per.entity.IncidentEvent</class>
		<class>com.jnoc.per.entity.Metier</class>
		<class>com.jnoc.per.entity.Nexus</class>
		<class>com.jnoc.per.entity.Permission</class>
		<class>com.jnoc.per.entity.Tag</class>
		<class>com.jnoc.per.entity.Telephone</class>
		<class>com.jnoc.per.entity.Ticket</class>
		<class>com.jnoc.per.entity.User</class>
		<class>com.jnoc.per.entity.Token</class>
		<class>com.jnoc.per.entity.Template</class>
		<class>com.jnoc.per.entity.DAS</class>
		<exclude-unlisted-classes>true</exclude-unlisted-classes>
		<properties>
			<property name="eclipselink.logging.level" value="SEVERE" />
		</properties>
	</persistence-unit>

</persistence>
